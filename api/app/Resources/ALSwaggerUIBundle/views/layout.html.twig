<!DOCTYPE html>
<html>
<head>
    <title>Documentation</title>

    {% block favicons %}
        {% block al_swagger_ui_favicons %}
        {% endblock %}
    {% endblock %}

    {% block stylesheets %}
        {% block al_swagger_ui_stylesheets %}
        {% endblock %}
        <style type="text/css">
            #header{
                background-color: #657EB4 !important;
            }
            #explore{
                background-color: #3E4D66 !important;
            }
            #header a#logo{
                background: url(/img/logo_white.png) no-repeat left center !important;
                padding: 20px 100px 20px 40px !important;
            }
            .deprecated{
                text-decoration: line-through !important;
            }
        </style>
    {% endblock %}

</head>

<body class="swagger-section">
<div id='header'>
    <div class="swagger-ui-wrap">
        <a id="logo" href="#"></a>
        <form id='api_selector'>
            <div class='input'><input placeholder="http://example.com/api" id="input_baseUrl" name="baseUrl" type="text" disabled /></div>
            <div class='input'><input placeholder="token" id="input_apiKey" name="apiKey" type="text"/></div>
            <div class='input'><a id="explore" href="#">Explore</a></div>
        </form>
    </div>
</div>

{% block al_swagger_ui_content %}
{% endblock %}

{% block javascripts %}
    {% block al_swagger_ui_javascripts %}
    {% endblock %}
{% endblock %}

</body>
</html>
