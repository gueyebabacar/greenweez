<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <title>JUMBLE</title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0">
  <!-- font -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Noto+Sans:400,700">
  <!--  General CSS  -->
  <style type="text/css">
    body[yahoo] {
      font-family: 'Noto Sans', arial, serif;
      width: 100%;
      padding: 0;
      margin: 0;
      /*letter-spacing: 1px;*/
      -webkit-font-smoothing: antialiased;
      mso-margin-top-alt: 0px;
      mso-margin-bottom-alt: 0px;
      mso-padding-alt: 0px 0px 0px 0px;
      background: #fff;
    }

    body[yahoo] .copyrights {
      margin-left: 12%;
    }

    body[yahoo] .logo__fb {
      margin-top: -8px;
      margin-left: 30px;
      vertical-align: middle;
    }

    body[yahoo] .table__ticket {
      border-style: solid;
      border-width: 1px;
      border-color: #b2b2b2;
      margin-bottom: 25px;
      margin-top: 20px;
      width: 70%;
    }

    body[yahoo] .table__ticket--separator {
      border-style: solid;
      border-width: 1px 0 0;
      border-color: #b2b2b2;
      padding-top: 10px;
      padding-bottom: 10px;
    }

    @media only screen and (max-width: 990px) {
      body[yahoo] .hide-mobile {
        display: none !important;
      }
    }

    @media only screen and (max-width: 640px) {
      body[yahoo] .copyrights {
        margin-bottom: 20px;
        margin-left: 0;
      }

      body[yahoo] .logo__fb {
        margin: 20px auto 0;
        display: block;
      }

      body[yahoo] .copyrights {
        display: block;
        margin-bottom: 20px;
        margin-left: 0;
        text-align: center;
      }

      body[yahoo] .table__ticket {
        width: 90%;
      }
    }
  </style>
</head>
<body yahoo="fix">
{% trans_default_domain "email" %}
<table cellpadding="10" cellspacing="0" border="0" width="100%" border-collapse="collapse" bgcolor="#fff">
  <tr>
    <td>
      <!-- email header -->
      <table border="0" cellpadding="40" cellspacing="0" width="100%" border-collapse="collapse">
        <tr>
          <td width="20%" height="10" class="hide-mobile">&nbsp;</td>
          <td width="60%" align="center" bgcolor="#000">
            <img src="data:image/png;base64," alt="Alt" border="0" width="200"/><br/>
          </td>
          <td width="20%" height="10" class="hide-mobile">&nbsp;</td>
        </tr>
      </table>
      <!-- /email header -->

      <!-- email main content -->
      {% block emailContent %}{% endblock %}
      <!-- /email main content -->

      <!-- email footer -->
      <table border="0" cellpadding="40" cellspacing="0" width="100%" border-collapse="collapse">
        <tr>
          <td width="20%" height="10" class="hide-mobile">&nbsp;</td>
          <td width="60%" bgcolor="#000">
            <strong class="copyrights"><font size="2" color="#fff">Email</font></strong>
            <a href="https://fr-fr.facebook.com/poivrerougefrance/" target="_blank"><img src="data:image/png;base64," alt="Alt" border="0" width="40" class="logo__fb" /></a>
          </td>
          <td width="20%" height="10" class="hide-mobile">&nbsp;</td>
        </tr>
      </table>
      <!-- /email footer -->
    </td>
  </tr>
</table>
</body>
</html>
